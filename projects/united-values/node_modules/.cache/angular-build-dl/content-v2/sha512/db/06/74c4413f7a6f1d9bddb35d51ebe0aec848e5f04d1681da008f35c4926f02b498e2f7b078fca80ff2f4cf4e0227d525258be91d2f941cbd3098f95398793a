  "./src/app/components/units/avia/aircrafts/aircrafts-specs-table/AircraftsSpecsTableItem.ts":
  /*!**************************************************************************************************!*\
    !*** ./src/app/components/units/avia/aircrafts/aircrafts-specs-table/AircraftsSpecsTableItem.ts ***!
    \**************************************************************************************************/

  /*! exports provided: Digest */

  /***/
  function srcAppComponentsUnitsAviaAircraftsAircraftsSpecsTableAircraftsSpecsTableItemTs(module, __webpack_exports__, __webpack_require__) {
    "use strict";

    __webpack_require__.r(__webpack_exports__);
    /* harmony export (binding) */


    __webpack_require__.d(__webpack_exports__, "Digest", function () {
      return Digest;
    });

    var Digest;

    (function (Digest) {
      function from(item) {
        if (!item) Error('no specs submitted');
        var digest = {
          id: item.unid,
          brand: item.designation.brand,
          name: designate(item.designation),
          firstFlight: item.firstFlight,
          isCargo: 'airlinerDerivative' in item,
          range: item.performance.range,
          cruiseSpeed: together(item.performance.cruiseSpeed),
          ceiling: item.performance.serviceCeiling,
          payload: together(item.weights.payload),
          cockpitCrew: item.cockpitCrew.length,
          powerplant: item.powerplant.length,
          price2020: item.listPrice2020
        };
        return digest;
      }

      Digest.from = from;

      function together(unitValue) {
        return "".concat(unitValue.value, " ").concat(unitValue.unit);
      }

      function designate(item) {
        if (!item) return '- none -';
        return "".concat(item.name ? item.name + " " : '').concat(item.family ? item.family : '').concat(item.serie ? item.serie : '').concat(item.variant ? "-" + item.variant : '').concat(item.modification ? item.modification : '');
      }
    })(Digest || (Digest = {}));
    /***/

  },

